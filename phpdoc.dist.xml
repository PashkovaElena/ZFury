<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>ZFury documentation</title>
    <parser>
        <target>../zfurydoc</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>../zfurydoc</target>

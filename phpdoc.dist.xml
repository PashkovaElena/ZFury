    <files>
        <directory>.</directory>
        <ignore>public/*</ignore>
        <ignore>vendor/*</ignore>
        <ignore>data/*</ignore>
        <ignore>config/*</ignore>
        <ignore>*/test/*</ignore>
    </files>
</phpdoc>